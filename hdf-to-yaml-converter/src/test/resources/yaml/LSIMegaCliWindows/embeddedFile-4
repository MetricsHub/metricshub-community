BEGIN {
    controllerID="" ; status="OK";statusInformation="";
      }

tolower($1) ~ /^bbu$/ && $2 ~ /^[Ss]tatus$/ && $4 ~ /^[Aa]dapter/ {controllerID=("#" $NF)}

tolower ($1) ~ /^issohgood/ {if (tolower($NF) ~ /no/) {status = "ALARM"}
                               print ("MSHW;"controllerID";" status";");
                               controllerID="" ; status="";
                              }