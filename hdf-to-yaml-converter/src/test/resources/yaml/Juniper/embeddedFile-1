BEGIN { OIDs["jnxProducts"] = "1.3.6.1.4.1.2636.1" }

$2 == "OBJECT" && $3 == "IDENTIFIER" {
  OIDName = $1
  OIDDescription = OIDName ; gsub(/^jnx/,"",OIDDescription)
  Parent = $6
  Value = $7
  ParentOID = OIDs[Parent]
  OIDs[OIDName] = ParentOID "." Value
  print "jnxContentsDescrTranslationTable(\"" OIDs[OIDName] ".0\")=\"" OIDDescription "\""
}