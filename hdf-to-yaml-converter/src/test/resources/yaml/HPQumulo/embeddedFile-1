BEGIN {FS = ": |,"}

/"id"/ {id = $2}
/"node_name"/ {displayId = $2}
/"model_number"/ {model = $2}
/"serial_number"/ {serial = $2}
/"node_status"/ {status = $2}
/"uuid"/ {
  uuid = $2;
  result = id ";" displayId ";" model ";" serial ";UUID: " uuid ";" status ";" status;
  gsub(/"/, "", result);
  print result
}
