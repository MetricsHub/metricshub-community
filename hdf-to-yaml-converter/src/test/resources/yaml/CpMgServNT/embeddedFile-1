BEGIN {FS="[;]"}
$1 ~ /[0-9]+/ && $2 ~ /[0-9]+/ {if (PowerConsumption[$1] == "") {PowerConsumption[$1] = $2}
                                   else {PowerConsumption[$1] = PowerConsumption[$1] + $2}
                               }
END { for (chassis in PowerConsumption) {print "MSHW;" chassis ";;;" PowerConsumption[chassis]}
    }