{
    if ($0 ~ /^c[0-9]+t[0-9A-Z]+d[0-9]+/ || $0 ~ /^s+d[0-9]+/)
    {
        if ($1 ~ /^c[0-9]+t[0-9A-Z]+d[0-9]+/)
        {
            targetIndex = index($1, "t")
            controller = substr($1, 1, targetIndex - 1)
        }
        else
        {
            controller = "Other"
        }
    }

    if ($0 ~/Illegal Request:/)
    {
        print "MSHW;" controller
    }
}