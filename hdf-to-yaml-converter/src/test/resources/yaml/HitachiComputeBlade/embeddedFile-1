BEGIN {FS="[;]"}
$1 ~ /P-[0-9]/ {Partition = $1 ; SlotNum = $2 ; BladeID = $3
                if (members[Partition] != "") { members[Partition] = members[Partition] " - " }
                members[Partition] = members[Partition] "(Blade-" BladeID "/Slot-" SlotNum ")"
                 }
END { for (Partition in members) {
             print "MSHW;" Partition ";Members = " members[Partition] ";"
            }
      }