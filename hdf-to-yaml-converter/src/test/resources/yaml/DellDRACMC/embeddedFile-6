($1 == "PWR" && $3 ~ /^PS-[0-9]$/ && NF == 6) {
    statusInformation = $4
    if ($6 == "Present")
    {
        statusInformation = statusInformation " - AC OK"
    }
    else
    {
        statusInformation = statusInformation " - AC NOK"
    }
    statusInformation = statusInformation " - " $5

    print "MSHW;" $3 ";" $5 ";" statusInformation ";"
}