$1 ~ /pool/ && $2 ~ /=/ {pool = $3}
$1 ~ /status/ && $2 ~ /=/ {status= $3}
$1 ~ /profile/ && $2 ~ /=/ && pool != "" {
                            profile= $3
                            print "MSHW;" pool ";" profile ";"
                            pool = "" ; status = ""
                           }