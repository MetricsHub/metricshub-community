BEGIN  {ID="";health="";controller="";}
$1 ~ /dev.sasd[0-9]/ {controller=$1 ;
                      gsub (/.dev./,"",controller) ;
                      }
$0 ~ /PHY ID/ {ID = $NF}
$0 ~ /PHY [Hh]ealth/ {health = $NF}
$0 ~ /Port SAS Address/ && ID != "" && $NF != "0x0" {
                                     print ("MSHW;" ID ";" health ";" controller ";");
                                     ID="";health=""
                                     }

