BEGIN {FS=";";}
{
       gsub("^0.?0*$","",$4)
       if ($4 == "" || $4 <= 0)
       {
        print  $1";"";"";"";"$5
       }
       else
       {
            if ($2 == "N/A" && $3 == "N/A" || $2 == "" && $3 == "" )
            {
            print $1";"";"";"";"$5
            }
            else
            {
            print $1";"$2";"$3";"$4";""NoCurrentReading"
            }
        }
}