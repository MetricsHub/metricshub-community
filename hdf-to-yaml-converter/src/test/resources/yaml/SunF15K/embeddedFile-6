BEGIN { totalPower = 0; }
($1 == "Current0") { current0 = $2; }
($1 == "Current1") { current1 = $2; }
($1 == "48VDC") {
    if (current0 != "" && current1 != "")
    {
        vdc = $2;
        totalPower = totalPower + (current0 + current1) * vdc;
    }
    current0 = "";
    current1 = "";
}
END { print "MSHW;PowerConsumption;" totalPower }