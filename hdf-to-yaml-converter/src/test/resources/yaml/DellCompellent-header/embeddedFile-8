BEGIN {FS=";";}
{
if ($4 ~ /^ScEnclosure/)
    {
         split($2,outputarray,".")
         enclosureID=outputarray[1]
         print $0 "true;"enclosureID ";Enclosure"
    }
else
        {
             split($2,outputarray,".")
             controllerID=outputarray[1]"."outputarray[2]
           if ( $5 == 0 || $5 == "")
           {
             gsub("0","",$5)
             print $0 "true;"controllerID";Blade"
           }
           else
           {
             print $0 ";"controllerID";Blade"
           }

         }
}