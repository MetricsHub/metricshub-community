$1 ~ /[0-9]+:/ && $2 ~ /[Ii][Dd]/ {
    DeviceID = $1 ;DeviceID = $1 ; gsub (/:/,"",DeviceID) ;
    DisplayID = $NF ;
    }

$1 ~ /[Hh]ealth/ && $2 ~ /[Ss]tate/ && DeviceID != "" {
    HealthValue = $NF ; gsub(/[()]/,"",HealthValue) ;
      print "MSHW;" DeviceID ";" DisplayID ";" HealthValue ";"
      DeviceID = "" ; DisplayID = "" ; HealthValue = ""
      }