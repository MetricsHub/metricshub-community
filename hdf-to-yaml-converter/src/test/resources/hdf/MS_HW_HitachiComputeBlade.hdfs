// HEADER

hdf.DisplayName="Hitachi Compute Blade Chassis"
hdf.TypicalPlatform="Hitachi Compute Blade"
hdf.ReliesOn="Hitachi Compute Blade Chassis Management Module"
hdf.Version="1.0"
hdf.Comments="This connector provides hardware monitoring for the Hitachi Compute Blade series chassis through the Hitachi Compute Blades Management Module"
hdf.RemoteSupport="true"
hdf.AppliesToOS="OOB"

//
// DETECTION
//

// Criteria 1: Must have a chassisConfigSystem entry
Detection.Criteria(1).Type="SNMP"
Detection.Criteria(1).SnmpGetNext="1.3.6.1.4.1.116.5.52.1.1.1"

//
// ENCLOSURES
//

//
// Discovery

// Enclosure (Chassis) chassisConfigSystem Table
// Name;ModelNumber;SerialNumber;ChassisID;
Enclosure.Discovery.Source(1).Type="SnmpTable"
Enclosure.Discovery.Source(1).SnmpTableOid="1.3.6.1.4.1.116.5.52.1.1.1"
Enclosure.Discovery.Source(1).SnmpTableSelectColumns="2,3,4,5"

// Add a key to do table joint
// MSHW;Name;ModelNumber;SerialNumber;ChassisID;
Enclosure.Discovery.Source(1).Compute(1).Type="LeftConcat"
Enclosure.Discovery.Source(1).Compute(1).Column=1
Enclosure.Discovery.Source(1).Compute(1).String="MSHW;"

// Get the Firmware Versions
// FirmwareVersion;EquipmentVersion;
Enclosure.Discovery.Source(2).Type="SnmpTable"
Enclosure.Discovery.Source(2).SnmpTableOid="1.3.6.1.4.1.116.5.52.1.1.5"
Enclosure.Discovery.Source(2).SnmpTableSelectColumns="1,2"

// Add a key to do table joint
// MSHW;FirmwareVersion;EquipmentVersion;
Enclosure.Discovery.Source(2).Compute(1).Type="LeftConcat"
Enclosure.Discovery.Source(2).Compute(1).Column=1
Enclosure.Discovery.Source(2).Compute(1).String="MSHW;"

// Add text to identify second firmware version
// MSHW;FirmwareVersion;EquipmentVersion;
Enclosure.Discovery.Source(2).Compute(2).Type="LeftConcat"
Enclosure.Discovery.Source(2).Compute(2).Column=3
Enclosure.Discovery.Source(2).Compute(2).String="Equipment Parameter Version: "

// Join the tables
// MSHW;Name;ModelNumber;SerialNumber;ChassisID;MSHW;FirmwareVersion;EquipmentVersion;
Enclosure.Discovery.Source(3).Type="TableJoint"
Enclosure.Discovery.Source(3).LeftTable=%Enclosure.Discovery.Source(1)%
Enclosure.Discovery.Source(3).RightTable=%Enclosure.Discovery.Source(2)%
Enclosure.Discovery.Source(3).LeftKeyColumn=1
Enclosure.Discovery.Source(3).RightKeyColumn=1
Enclosure.Discovery.Source(3).DefaultRightLine=";;;"

// The InstanceTable
//   1    2       3           4           5       6          7            8
// MSHW;Name;ModelNumber;SerialNumber;ChassisID;MSHW;FirmwareVersion;EquipmentVersion;
Enclosure.Discovery.InstanceTable=%Enclosure.Discovery.Source(3)%
Enclosure.Discovery.Instance.DeviceID=InstanceTable.Column(5)
Enclosure.Discovery.Instance.Vendor=InstanceTable.Column(2)
Enclosure.Discovery.Instance.Model=InstanceTable.Column(3)
Enclosure.Discovery.Instance.Type="Blade Chassis"
Enclosure.Discovery.Instance.SerialNumber=InstanceTable.Column(4)
Enclosure.Discovery.Instance.BiosVersion=InstanceTable.Column(7)
Enclosure.Discovery.Instance.AdditionalInformation1=InstanceTable.Column(8)

//
// Collect

Enclosure.Collect.Type="MultiInstance"

// Get the various enclosures (Chassis)
// PowerConsumptionKW
Enclosure.Collect.Source(1).Type="SnmpGet"
Enclosure.Collect.Source(1).SnmpOid="1.3.6.1.4.1.116.5.52.1.1.4.9.0"

// Add a key to do table joint
// MSHW;PowerConsumptionKW;
Enclosure.Collect.Source(1).Compute(1).Type="LeftConcat"
Enclosure.Collect.Source(1).Compute(1).Column=1
Enclosure.Collect.Source(1).Compute(1).String="MSHW;"

// Join with the discovery table to get the ID
// MSHW;Name;ModelNumber;SerialNumber;ChassisID;MSHW;PowerConsumptionKW;
Enclosure.Collect.Source(2).Type="TableJoint"
Enclosure.Collect.Source(2).LeftTable=%Enclosure.Discovery.Source(1)%
Enclosure.Collect.Source(2).RightTable=%Enclosure.Collect.Source(1)%
Enclosure.Collect.Source(2).LeftKeyColumn=1
Enclosure.Collect.Source(2).RightKeyColumn=1
Enclosure.Collect.Source(2).DefaultRightLine=";;"

// ValueTable = Source(1)
//   1    2     3             4          5        6        7
// MSHW;Name;ModelNumber;SerialNumber;ChassisID;MSHW;PowerConsumptionKW;
Enclosure.Collect.ValueTable=%Enclosure.Collect.Source(2)%
Enclosure.Collect.DeviceID=ValueTable.Column(5)
Enclosure.Collect.PowerConsumption=ValueTable.Column(7)

//
// BLADES
//

// This is going to be a mess as all blades have their own table

// Source(1) = Blade 1
// ModelNumber;SerialNumber;Version;Manufacturer;Model;
Blade.Discovery.Source(1).Type="SnmpTable"
Blade.Discovery.Source(1).SnmpTableOid="1.3.6.1.4.1.116.5.52.1.2.1.1.1"
Blade.Discovery.Source(1).SnmpTableSelectColumns="2,3,4,5,7"

// Add a Blade Number
// ID;Model;SerialNumber;Version;Manufacturer;Model;
Blade.Discovery.Source(1).Compute(1).Type="LeftConcat"
Blade.Discovery.Source(1).Compute(1).Column=1
Blade.Discovery.Source(1).Compute(1).String="1;"

// Source(2) = Blade 2
// ModelNumber;SerialNumber;Version;Manufacturer;Model;
Blade.Discovery.Source(2).Type="SnmpTable"
Blade.Discovery.Source(2).SnmpTableOid="1.3.6.1.4.1.116.5.52.1.2.1.2.1"
Blade.Discovery.Source(2).SnmpTableSelectColumns="2,3,4,5,7"

// Add a Blade Number
// ID;Model;SerialNumber;Version;Manufacturer;Model;
Blade.Discovery.Source(2).Compute(1).Type="LeftConcat"
Blade.Discovery.Source(2).Compute(1).Column=1
Blade.Discovery.Source(2).Compute(1).String="2;"

// Source(3) = Blade 3
// ModelNumber;SerialNumber;Version;Manufacturer;Model;
Blade.Discovery.Source(3).Type="SnmpTable"
Blade.Discovery.Source(3).SnmpTableOid="1.3.6.1.4.1.116.5.52.1.2.1.3.1"
Blade.Discovery.Source(3).SnmpTableSelectColumns="2,3,4,5,7"

// Add a Blade Number
// ID;Model;SerialNumber;Version;Manufacturer;Model;
Blade.Discovery.Source(3).Compute(1).Type="LeftConcat"
Blade.Discovery.Source(3).Compute(1).Column=1
Blade.Discovery.Source(3).Compute(1).String="3;"

// Source(4) = Blade 4
// ModelNumber;SerialNumber;Version;Manufacturer;Model;
Blade.Discovery.Source(4).Type="SnmpTable"
Blade.Discovery.Source(4).SnmpTableOid="1.3.6.1.4.1.116.5.52.1.2.1.4.1"
Blade.Discovery.Source(4).SnmpTableSelectColumns="2,3,4,5,7"

// Add a Blade Number
// ID;Model;SerialNumber;Version;Manufacturer;Model;
Blade.Discovery.Source(4).Compute(1).Type="LeftConcat"
Blade.Discovery.Source(4).Compute(1).Column=1
Blade.Discovery.Source(4).Compute(1).String="4;"

// Source(5) = Blade 5
// ModelNumber;SerialNumber;Version;Manufacturer;Model;
Blade.Discovery.Source(5).Type="SnmpTable"
Blade.Discovery.Source(5).SnmpTableOid="1.3.6.1.4.1.116.5.52.1.2.1.5.1"
Blade.Discovery.Source(5).SnmpTableSelectColumns="2,3,4,5,7"

// Add a Blade Number
// ID;Model;SerialNumber;Version;Manufacturer;Model;
Blade.Discovery.Source(5).Compute(1).Type="LeftConcat"
Blade.Discovery.Source(5).Compute(1).Column=1
Blade.Discovery.Source(5).Compute(1).String="5;"

// Source(6) = Blade 6
// ModelNumber;SerialNumber;Version;Manufacturer;Model;
Blade.Discovery.Source(6).Type="SnmpTable"
Blade.Discovery.Source(6).SnmpTableOid="1.3.6.1.4.1.116.5.52.1.2.1.6.1"
Blade.Discovery.Source(6).SnmpTableSelectColumns="2,3,4,5,7"

// Add a Blade Number
// ID;Model;SerialNumber;Version;Manufacturer;Model;
Blade.Discovery.Source(6).Compute(1).Type="LeftConcat"
Blade.Discovery.Source(6).Compute(1).Column=1
Blade.Discovery.Source(6).Compute(1).String="6;"

// Source(7) = Blade 7
// ModelNumber;SerialNumber;Version;Manufacturer;Model;
Blade.Discovery.Source(7).Type="SnmpTable"
Blade.Discovery.Source(7).SnmpTableOid="1.3.6.1.4.1.116.5.52.1.2.1.7.1"
Blade.Discovery.Source(7).SnmpTableSelectColumns="2,3,4,5,7"

// Add a Blade Number
// ID;Model;SerialNumber;Version;Manufacturer;Model;
Blade.Discovery.Source(7).Compute(1).Type="LeftConcat"
Blade.Discovery.Source(7).Compute(1).Column=1
Blade.Discovery.Source(7).Compute(1).String="7;"

// Source(8) = Blade 8
// ModelNumber;SerialNumber;Version;Manufacturer;Model;
Blade.Discovery.Source(8).Type="SnmpTable"
Blade.Discovery.Source(8).SnmpTableOid="1.3.6.1.4.1.116.5.52.1.2.1.8.1"
Blade.Discovery.Source(8).SnmpTableSelectColumns="2,3,4,5,7"

// Add a Blade Number
// ID;Model;SerialNumber;Version;Manufacturer;Model;
Blade.Discovery.Source(8).Compute(1).Type="LeftConcat"
Blade.Discovery.Source(8).Compute(1).Column=1
Blade.Discovery.Source(8).Compute(1).String="8;"

// Join all these tables together
// ID;Model;SerialNumber;Version;Manufacturer;Model;
Blade.Discovery.Source(9).Type="TableUnion"
Blade.Discovery.Source(9).Table1=%Blade.Discovery.Source(1)%
Blade.Discovery.Source(9).Table2=%Blade.Discovery.Source(2)%
Blade.Discovery.Source(9).Table3=%Blade.Discovery.Source(3)%
Blade.Discovery.Source(9).Table4=%Blade.Discovery.Source(4)%
Blade.Discovery.Source(9).Table5=%Blade.Discovery.Source(5)%

Blade.Discovery.Source(10).Type="TableUnion"
Blade.Discovery.Source(10).Table1=%Blade.Discovery.Source(9)%
Blade.Discovery.Source(10).Table2=%Blade.Discovery.Source(6)%
Blade.Discovery.Source(10).Table3=%Blade.Discovery.Source(7)%
Blade.Discovery.Source(10).Table4=%Blade.Discovery.Source(8)%

// Exclude blades with a model of "Not Install"
// ID;Model;SerialNumber;Version;Manufacturer;Model;
Blade.Discovery.Source(10).Compute(1).Type="ExcludeMatchingLines"
Blade.Discovery.Source(10).Compute(1).Column=2
Blade.Discovery.Source(10).Compute(1).ValueList="Not Install"

// Identify the Version
// ID;ModelNumber;SerialNumber;Version;Manufacturer;Model;
Blade.Discovery.Source(10).Compute(2).Type="LeftConcat"
Blade.Discovery.Source(10).Compute(2).Column=2
Blade.Discovery.Source(10).Compute(2).String="Part Number: "

// Identify the Version
// ID;ModelNumber;SerialNumber;Version;Manufacturer;Model;
Blade.Discovery.Source(10).Compute(3).Type="LeftConcat"
Blade.Discovery.Source(10).Compute(3).Column=4
Blade.Discovery.Source(10).Compute(3).String="Version: "

// Special Character sequence for joining AdditionalInformation
Blade.Discovery.Source(10).Compute(4).Type="RightConcat"
Blade.Discovery.Source(10).Compute(4).Column=2
Blade.Discovery.Source(10).Compute(4).String=" - "

// Join Part Number and version because we ran out of AdditionalInformation slots
Blade.Discovery.Source(10).Compute(5).Type="RightConcat"
Blade.Discovery.Source(10).Compute(5).Column=2
Blade.Discovery.Source(10).Compute(5).String=Column(4)

// OK, here we go again, get the slot numbers

// Source 11 = Blade 1 Slot
// Slot
Blade.Discovery.Source(11).Type="SnmpGet"
Blade.Discovery.Source(11).SnmpOid="1.3.6.1.4.1.116.5.52.1.2.1.1.4.1.0"

// Add a Blade Number
// ID;Slot;
Blade.Discovery.Source(11).Compute(1).Type="LeftConcat"
Blade.Discovery.Source(11).Compute(1).Column=1
Blade.Discovery.Source(11).Compute(1).String="1;"

// Source 12 = Blade 2 Slot
// Slot
Blade.Discovery.Source(12).Type="SnmpGet"
Blade.Discovery.Source(12).SnmpOid="1.3.6.1.4.1.116.5.52.1.2.1.2.4.1.0"

// Add a Blade Number
// ID;Slot;
Blade.Discovery.Source(12).Compute(1).Type="LeftConcat"
Blade.Discovery.Source(12).Compute(1).Column=1
Blade.Discovery.Source(12).Compute(1).String="2;"

// Source 13 = Blade 3 Slot
// Slot
Blade.Discovery.Source(13).Type="SnmpGet"
Blade.Discovery.Source(13).SnmpOid="1.3.6.1.4.1.116.5.52.1.2.1.3.4.1.0"

// Add a Blade Number
// ID;Slot;
Blade.Discovery.Source(13).Compute(1).Type="LeftConcat"
Blade.Discovery.Source(13).Compute(1).Column=1
Blade.Discovery.Source(13).Compute(1).String="3;"

// Source 14 = Blade 4 Slot
// Slot
Blade.Discovery.Source(14).Type="SnmpGet"
Blade.Discovery.Source(14).SnmpOid="1.3.6.1.4.1.116.5.52.1.2.1.4.4.1.0"

// Add a Blade Number
// ID;Slot;
Blade.Discovery.Source(14).Compute(1).Type="LeftConcat"
Blade.Discovery.Source(14).Compute(1).Column=1
Blade.Discovery.Source(14).Compute(1).String="4;"

// Source 15 = Blade 5 Slot
// Slot
Blade.Discovery.Source(15).Type="SnmpGet"
Blade.Discovery.Source(15).SnmpOid="1.3.6.1.4.1.116.5.52.1.2.1.5.4.1.0"

// Add a Blade Number
// ID;Slot;
Blade.Discovery.Source(15).Compute(1).Type="LeftConcat"
Blade.Discovery.Source(15).Compute(1).Column=1
Blade.Discovery.Source(15).Compute(1).String="5;"

// Source 16 = Blade 6 Slot
// Slot
Blade.Discovery.Source(16).Type="SnmpGet"
Blade.Discovery.Source(16).SnmpOid="1.3.6.1.4.1.116.5.52.1.2.1.6.4.1.0"

// Add a Blade Number
// ID;Slot;
Blade.Discovery.Source(16).Compute(1).Type="LeftConcat"
Blade.Discovery.Source(16).Compute(1).Column=1
Blade.Discovery.Source(16).Compute(1).String="6;"

// Source 17 = Blade 7 Slot
// Slot
Blade.Discovery.Source(17).Type="SnmpGet"
Blade.Discovery.Source(17).SnmpOid="1.3.6.1.4.1.116.5.52.1.2.1.7.4.1.0"

// Add a Blade Number
// ID;Slot;
Blade.Discovery.Source(17).Compute(1).Type="LeftConcat"
Blade.Discovery.Source(17).Compute(1).Column=1
Blade.Discovery.Source(17).Compute(1).String="7;"

// Source 18 = Blade 8 Slot
// Slot
Blade.Discovery.Source(18).Type="SnmpGet"
Blade.Discovery.Source(18).SnmpOid="1.3.6.1.4.1.116.5.52.1.2.1.8.4.1.0"

// Add a Blade Number
// ID;Slot;
Blade.Discovery.Source(18).Compute(1).Type="LeftConcat"
Blade.Discovery.Source(18).Compute(1).Column=1
Blade.Discovery.Source(18).Compute(1).String="8;"

// Join all these tables together
// **Watch out - This table is re-used in the otherdevice discovery**
// ID;Slot;
Blade.Discovery.Source(19).Type="TableUnion"
Blade.Discovery.Source(19).Table1=%Blade.Discovery.Source(11)%
Blade.Discovery.Source(19).Table2=%Blade.Discovery.Source(12)%
Blade.Discovery.Source(19).Table3=%Blade.Discovery.Source(13)%
Blade.Discovery.Source(19).Table4=%Blade.Discovery.Source(14)%
Blade.Discovery.Source(19).Table5=%Blade.Discovery.Source(15)%

Blade.Discovery.Source(20).Type="TableUnion"
Blade.Discovery.Source(20).Table1=%Blade.Discovery.Source(19)%
Blade.Discovery.Source(20).Table2=%Blade.Discovery.Source(16)%
Blade.Discovery.Source(20).Table3=%Blade.Discovery.Source(17)%
Blade.Discovery.Source(20).Table4=%Blade.Discovery.Source(18)%

// Now let's join the main discovery table with the slot table
// ID;ModelNumber;SerialNumber;Version;Manufacturer;Model;ID;Slot;
Blade.Discovery.Source(21).Type="TableJoint"
Blade.Discovery.Source(21).LeftTable=%Blade.Discovery.Source(10)%
Blade.Discovery.Source(21).RightTable=%Blade.Discovery.Source(20)%
Blade.Discovery.Source(21).LeftKeyColumn=1
Blade.Discovery.Source(21).RightKeyColumn=1
Blade.Discovery.Source(21).DefaultRightLine=";;"

// Duplicate the ID
// ID;DisplayID;ModelNumber;SerialNumber;Version;Manufacturer;Model;ID;Slot;
Blade.Discovery.Source(21).Compute(1).Type="DuplicateColumn"
Blade.Discovery.Source(21).Compute(1).Column=1

// Add a Blade-
// ID;DisplayID;ModelNumber;SerialNumber;Version;Manufacturer;Model;ID;Slot;
Blade.Discovery.Source(21).Compute(2).Type="LeftConcat"
Blade.Discovery.Source(21).Compute(2).Column=2
Blade.Discovery.Source(21).Compute(2).String="Blade-"

// Add Location: to AdditionalInformation
// ID;DisplayID;ModelNumber;SerialNumber;Version;Manufacturer;Model;ID;Slot;
Blade.Discovery.Source(21).Compute(3).Type="LeftConcat"
Blade.Discovery.Source(21).Compute(3).Column=9
Blade.Discovery.Source(21).Compute(3).String="Location: "

// Add Vendor: to AdditionalInformation
