Blade.Discovery.Source(21).Compute(4).Column=6
Blade.Discovery.Source(21).Compute(4).String="Vendor: "

// InstanceTable = Source(1)
// ID;DisplayID;PartNumber - Version;SerialNumber;Version;Manufacturer;Model;ID;Slot;
Blade.Discovery.InstanceTable=%Blade.Discovery.Source(21)%
Blade.Discovery.Instance.DeviceID=InstanceTable.Column(1)
