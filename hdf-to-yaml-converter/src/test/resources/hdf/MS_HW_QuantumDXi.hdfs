//
// Discovery

// Source(1)
Enclosure.Discovery.Source(1).Type="OSCommand"
Enclosure.Discovery.Source(1).CommandLine="syscli --getstatus storagearray"

Enclosure.Discovery.Source(2).Type="OSCommand"
Enclosure.Discovery.Source(2).CommandLine="syscli --getstatus systemboard"

Enclosure.Discovery.Source(3).Type="OSCommand"
Enclosure.Discovery.Source(3).CommandLine="for array in `syscli --getstatus storagearray | grep -oP '(?<=Name = )[\w\d]+'`; do echo MSHW_START $array;syscli --getstatus storagearraydetails --name $array ; echo MSHW_END $array; done"

Enclosure.Discovery.Source(4).Type="OSCommand"
Enclosure.Discovery.Source(4).CommandLine="syscli --getstatus hostbusadapter"

Enclosure.Discovery.Source(5).Type="TableUnion"
Enclosure.Discovery.Source(5).Table1=%Enclosure.Discovery.Source(1)%
Enclosure.Discovery.Source(5).Table2=%Enclosure.Discovery.Source(2)%
Enclosure.Discovery.Source(5).Table3=%Enclosure.Discovery.Source(3)%
Enclosure.Discovery.Source(5).Table4=%Enclosure.Discovery.Source(4)%

// Run the Command Outputs through an AWK Script
// MSHW;ID;Type;Name;Value;Status;StatusInformation;
// MSHW;ID;Type;Name;Value;Status;StatusInformation;Model;Configuration;SystemVersion;
Enclosure.Discovery.Source(5).Compute(1).Type="Awk"
Enclosure.Discovery.Source(5).Compute(1).AwkScript=EmbeddedFile(1)
Enclosure.Discovery.Source(5).Compute(1).Separators=";"
Enclosure.Discovery.Source(5).Compute(1).KeepOnlyRegExp="^MSHW;"

// Make a copy of the discovery
Enclosure.Discovery.Source(6)=%Enclosure.Discovery.Source(5)%

// Add "Location: " to AdditionalInformation
Enclosure.Discovery.Source(6).Compute(1).Type="LeftConcat"
Enclosure.Discovery.Source(6).Compute(1).Column=8
Enclosure.Discovery.Source(6).Compute(1).String="Location: "


Enclosure.Discovery.Source(7)=%Enclosure.Discovery.Source(5)%

// Keep only "Enclosure"
// MSHW;ID;Type;Name;Value;Status;StatusInformation;Model;Configuration;SystemVersion;
Enclosure.Discovery.Source(7).Compute(1).Type="KeepOnlyMatchingLines"
Enclosure.Discovery.Source(7).Compute(1).Column=3
Enclosure.Discovery.Source(7).Compute(1).RegExp="Enclosure"

// Add "Configuration: " to AdditionalInformation1
Enclosure.Discovery.Source(7).Compute(2).Type="LeftConcat"
Enclosure.Discovery.Source(7).Compute(2).Column=9
Enclosure.Discovery.Source(7).Compute(2).String="Configuration: "

// Instance Table
// MSHW;ID;Type;Name;Value;Status;StatusInformation;Model;Configuration;SystemVersion;
Enclosure.Discovery.InstanceTable=%Enclosure.Discovery.Source(7)%
Enclosure.Discovery.Instance.DeviceID=InstanceTable.Column(4)
Enclosure.Discovery.Instance.DisplayID=InstanceTable.Column(4)
Enclosure.Discovery.Instance.Type="Storage"
Enclosure.Discovery.Instance.Model=InstanceTable.Column(8)
Enclosure.Discovery.Instance.AdditionalInformation1=InstanceTable.Column(9)
Enclosure.Discovery.Instance.BiosVersion=InstanceTable.Column(10)

//
// Collect

// Collect type is multi-instance (execute once)
Enclosure.Collect.Type="MultiInstance"

Enclosure.Collect.Source(1).Type="OSCommand"
Enclosure.Collect.Source(1).CommandLine="syscli --getstatus storagearray"

Enclosure.Collect.Source(2).Type="OSCommand"
Enclosure.Collect.Source(2).CommandLine="syscli --getstatus systemboard"

Enclosure.Collect.Source(3).Type="OSCommand"
Enclosure.Collect.Source(3).CommandLine="for array in `syscli --getstatus storagearray | grep -oP '(?<=Name = )[\w\d]+'`; do echo MSHW_START $array;syscli --getstatus storagearraydetails --name $array ; echo MSHW_END $array; done"

Enclosure.Collect.Source(4).Type="OSCommand"
Enclosure.Collect.Source(4).CommandLine="syscli --getstatus hostbusadapter"

Enclosure.Collect.Source(5).Type="TableUnion"
Enclosure.Collect.Source(5).Table1=%Enclosure.Discovery.Source(1)%
Enclosure.Collect.Source(5).Table2=%Enclosure.Discovery.Source(2)%
Enclosure.Collect.Source(5).Table3=%Enclosure.Discovery.Source(3)%
Enclosure.Collect.Source(5).Table4=%Enclosure.Discovery.Source(4)%

// Run the Command Outputs through an AWK Script
// MSHW;ID;Type;Name;Value;Status;StatusInformation;
// MSHW;ID;Type;Name;Value;Status;StatusInformation;Model;Configuration;SystemVersion;
Enclosure.Collect.Source(5).Compute(1).Type="Awk"
Enclosure.Collect.Source(5).Compute(1).AwkScript=EmbeddedFile(1)
Enclosure.Collect.Source(5).Compute(1).Separators=";"
Enclosure.Collect.Source(5).Compute(1).KeepOnlyRegExp="^MSHW;"


// Make a copy of the discovery
Enclosure.Collect.Source(6)=%Enclosure.Collect.Source(5)%

// Keep only "Enclosure"
// MSHW;ID;Type;Name;Value;Status;StatusInformation;Model;Configuration;SystemVersion;
Enclosure.Collect.Source(6).Compute(1).Type="KeepOnlyMatchingLines"
Enclosure.Collect.Source(6).Compute(1).Column=3
Enclosure.Collect.Source(6).Compute(1).RegExp="Enclosure"

// Translate Status into PATROLStatus
// MSHW;ID;Type;Name;Value;PatrolStatus;StatusInformation;Model;Configuration;SystemVersion;
Enclosure.Collect.Source(6).Compute(2).Type="Translate"
Enclosure.Collect.Source(6).Compute(2).Column=6
Enclosure.Collect.Source(6).Compute(2).TranslationTable="StatusTranslationTable"

// The StatusTranslationTable
StatusTranslationTable("Normal")="OK"
StatusTranslationTable("Ready")="OK"
StatusTranslationTable("Running")="OK"
StatusTranslationTable("Up")="OK"
StatusTranslationTable("Down")="WARN"
StatusTranslationTable("Attention")="WARN"
StatusTranslationTable("Initializing")="WARN"
StatusTranslationTable("Warning")="WARN"
StatusTranslationTable("Rebuilding")="WARN"
StatusTranslationTable("Failure")="ALARM"
StatusTranslationTable("NA")="UNKNOWN"
StatusTranslationTable("Online")="OK"
// Global Hot Spare
StatusTranslationTable("Global")="OK"
StatusTranslationTable("Unknown")="UNKNOWN"
StatusTranslationTable(Default)="ALARM"
StatusTranslationTable("Unassigned")="OK"


// ValueTable = Source(1)
Enclosure.Collect.ValueTable=%Enclosure.Collect.Source(6)%
Enclosure.Collect.DeviceID=ValueTable.Column(4)
Enclosure.Collect.Status=ValueTable.Column(6)
Enclosure.Collect.StatusInformation=ValueTable.Column(7)

